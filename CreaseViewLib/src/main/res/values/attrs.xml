<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CreaseView">

        <attr name="numBackground" format="reference" />
        <attr name="numEditable" format="boolean" />
        <attr name="decreaseIcon" format="reference" />
        <attr name="increaseIcon" format="reference" />
        <attr name="buttonTextColor" format="reference|color"/>

    </declare-styleable>


    <declare-styleable name="CreaseButton">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="textDrawable" format="reference" />
    </declare-styleable>
</resources>